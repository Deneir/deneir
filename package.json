{
  "name": "deneir",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "d3": "^5.12.0",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-scripts": "3.4.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint src",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/reducers/*.{js,jsx}",
      "src/actions/*.{js,jsx}",
      "src/services/*.{js,jsx}",
      "src/**/reducers/*.{js,jsx}",
      "src/**/actions/*.{js,jsx}",
      "!src/reducers/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
